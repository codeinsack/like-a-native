{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-use-before-define": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-useless-escape": "off",
    "camelcase": "off"
  }
}
